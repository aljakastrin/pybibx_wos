@article{ WOS:000466842600002,
Author = {Yu, Ting and Li, Lei and Liu, Wenyan and Ya, Bailiu and Cheng, Hongju
   and Xin, Qing},
Title = {Silencing of NADPH Oxidase 4 Attenuates Hypoxia Resistance in
   Neuroblastoma Cells SH-SY5Y by Inhibiting PI3K/Akt-Dependent Glycolysis},
Journal = {ONCOLOGY RESEARCH},
