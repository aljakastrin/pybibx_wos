@article{ WOS:000523590100001,
Author = {Cassina, Laura and Chiaravalli, Marco and Boletta, Alessandra},
Title = {Increased mitochondrial fragmentation in polycystic kidney disease acts
   as a modifier of disease progression},
Journal = {FASEB JOURNAL},
Year = {2020},
Volume = {34},
