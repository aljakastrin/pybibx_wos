@article{ WOS:000465093900005,
Author = {Feng, Yuhua and Lin, Jinguan and Liu, Yiping and Tang, Youhong and Zhou,
   Yangying and Zhong, Meizuo},
Title = {Investigation of expressions of PDK1, PLK1 and c-Myc in diffuse large
   B-cell lymphoma},
