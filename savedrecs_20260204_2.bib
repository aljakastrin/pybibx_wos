@article{ WOS:000759919100001,
Author = {Takeuchi, Yukiko and Saino, Orie and Okinaka, Yuka and Ogawa, Yuko and
   Akamatsu, Rie and Kikuchi-Taura, Akie and Kataoka, Yosky and Maeda,
   Mitsuyo and Gul, Sheraz and Claussen, Carsten and Boltze, Johannes and
