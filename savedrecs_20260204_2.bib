@article{ WOS:000615918600004,
Author = {Kolb, David and Kolishetti, Nagesh and Surnar, Bapurao and Sarkar,
   Shrita and Guin, Subham and Shah, Anuj S. and Dhar, Shanta},
Title = {Metabolic Modulation of the Tumor Microenvironment Leads to Multiple
   Checkpoint Inhibition and Immune Cell Infiltration},
Journal = {ACS NANO},
