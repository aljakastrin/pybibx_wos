@article{ WOS:000538199100013,
Author = {Jothi, Jayalakshmi and Janardhanam, Vanisree Arambakkam and Rama, K.},
Title = {Connexin 30 mediated rewiring of glucose metabolism in rat C6 xenograft
   and grades of glioma},
