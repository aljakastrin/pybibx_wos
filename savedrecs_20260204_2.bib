@article{ WOS:000582678700011,
Author = {Sakle, Nikhil S. and More, Shweta A. and Mokale, Santosh N.},
Title = {A network pharmacology-based approach to explore potential targets of
   Caesalpinia pulcherima: an updated prototype in drug discovery},
Journal = {SCIENTIFIC REPORTS},
