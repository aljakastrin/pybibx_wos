@article{ WOS:000531216100002,
Author = {Zhu, Jinfeng and Zheng, Guihua and Xu, Haifei and Jin, Xiaoxia and Tang,
   Ting and Wang, Xiaolin},
Title = {Expression and prognostic significance of pyruvate dehydrogenase kinase
   1 in bladder urothelial carcinoma},
Journal = {VIRCHOWS ARCHIV},
